{
  "name": "mindy/page-bundle",
  "license": "MIT",
  "require": {
    "mindy/orm-bundle": "~3.0",
    "mindy/pagination-bundle": "~3.0",
    "mindy/sitemap-bundle": "~3.0",
    "mindy/template-bundle": "~3.0",
    "mindy/mindy-bundle": "~3.0",
    "mindy/ckeditor-bundle": "~3.0"
  },
  "suggest": {
    "mindy/admin-bundle": "The Administrator panel for Mindy Framework"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Mindy\\Bundle\\PageBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mindy\\Bundle\\PageBundle\\Tests\\": "./Tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  }
}
