  "require": {
    "php": ">=5.5.9",
    "imagine/imagine": "*",
    "doctrine/dbal": "~2.0",
    "symfony/validator": "~3.0",
    "mindy/query_builder": "^2.0",
    "cocur/slugify": "~2.0",
    "league/flysystem": "~1.0",
    "mindy/validation": "~2.0"
  },
  "authors": [
    {
      "name": "Maxim Falaleev",
      "email": "max@studio107.ru",
      "homepage": "http://studio107.ru"
    }
  ],
  "support": {
    "issues": "https://github.com/MindyPHP/Mindy/issues?state=open"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.3"
  }
}
