{
  "name": "mindy/orm",
  "homepage": "https://github.com/MindyPHP/Mindy_Orm",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Mindy\\Orm\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
